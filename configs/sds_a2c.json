{
  "env": [
    {
      "name": [["small_dense_short"]],
      "num_envs": [512],
      "train_steps": [3e7]
    }
  ],
  "agent": [
    {
      "name": ["A2C"],
      "gae_lambda": [0.95],
      "rollout_steps": [20],
      "critic_loss_weight": [0.5],
      "entropy_weight": [0.01]
    }
  ],
  "agent_optimizer": [
    {
      "name": ["RMSProp", "Adam"],
      "kwargs": [
        {
          "learning_rate": [3e-2, 1e-2, 3e-3, 1e-3, 3e-4, 1e-4],
          "gradient_clip": [1]
        }
      ]
    }
  ],
  "discount": [0.995],
  "seed": [42],
  "generate_random_seed": [true]
}
