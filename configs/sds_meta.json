{
  "env": [
    {
      "name": [["small_dense_long"]],
      "num_envs": [512],
      "train_steps": [3e7]
    }
  ],
  "agent": [
    {
      "name": ["MetaA2C"],
      "inner_updates": [4],
      "gae_lambda": [0.95],
      "rollout_steps": [20],
      "critic_loss_weight": [0.5],
      "entropy_weight": [0.01],
      "reset_interval": [256, 512]
    }
  ],
  "agent_optimizer": [
    {
      "name": ["Optim4RL", "LinearOptim", "L2LGD2"],
      "kwargs": [
        {
          "mlp_dims": [[16, 16]],
          "hidden_size": [8],
          "param_load_path": [""],
          "learning_rate": [1e-2],
          "gradient_clip": [1]
        }
      ]
    }
  ],
  "meta_optimizer": [
    {
      "name": ["Adam"],
      "kwargs": [
        {
          "learning_rate": [3e-5, 1e-4, 3e-4, 1e-3],
          "gradient_clip": [1]
        }
      ]
    }
  ],
  "discount": [0.995],
  "seed": [42],
  "save_param": [256],
  "display_interval": [50],
  "generate_random_seed": [true]
}
